{
  "paths": {
    "core/*":["./core"]
  },
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
		"outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@core/*":[
        "./core/*"
      ]
    }
	},
  "exclude": ["node_modules", "dist", "ds", "test"], 
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  },
  "plugins": [{
    "transform": "typescript-transform-paths",
  }]
}